{
    "name": "pportelette/crud-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides basic CRUD endpoints for Doctrine entities",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Pierre Portelette",
            "email": "pierre@cloudnite.net"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "doctrine/doctrine-bundle": "^2.7",
        "pportelette/pageable-bundle": "^0.2",
        "symfony/framework-bundle": "^5.3|^6.0|^7.0",
        "symfony/serializer": "^5.3|^6.0|^7.0",
        "symfony/yaml": "^5.3|^6.0|^7.0"
    },
    "require-dev": {
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Pportelette\\CrudBundle\\": "src/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
